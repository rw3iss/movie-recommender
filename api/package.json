{
  "name": "movie-recommendation-backend",
  "version": "1.0.0",
  "description": "Fastify backend for movie recommendation PWA",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "setup": "tsx src/setup.ts",
    "migrate": "tsx scripts/migrate.ts",
    "migrate:create": "tsx scripts/create-migration.ts",
    "seed": "tsx scripts/seed.ts",
    "docs": "typedoc --out docs src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@fastify/compress": "^6.5.0",
    "@fastify/cookie": "^9.2.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/etag": "^4.2.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.3",
    "@fastify/multipart": "^8.0.0",
    "@fastify/postgres": "^5.2.2",
    "@fastify/rate-limit": "^9.0.1",
    "@fastify/redis": "^6.1.1",
    "@fastify/static": "^6.12.0",
    "@fastify/type-provider-typebox": "^3.5.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "csv-parse": "^5.5.2",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "fastify-plugin": "^4.5.1",
    "ml-matrix": "^6.10.7",
    "natural": "^6.10.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@sinclair/typebox": "^0.34.37",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "movies",
    "recommendations",
    "fastify",
    "api",
    "postgresql",
    "redis"
